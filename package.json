    "ahooks": "^2.10.9",
    "antd": "^4.16.8",
    "connect-history-api-fallback": "^1.6.0",
    "express": "^4.17.1",
    "react": "17.x",
    "react-dom": "17.x",
    "umi": "^3.4.25"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.6.2",
    "@umijs/plugin-esbuild": "^1.3.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.14",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.0.7",
